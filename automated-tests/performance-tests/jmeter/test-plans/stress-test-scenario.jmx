        <stringProp name="ThreadGroup.num_threads">${INITIAL_USERS}</stringProp>
        <stringProp name="Threads initial delay">0</stringProp>
        <stringProp name="Start users count">${INITIAL_USERS}</stringProp>
        <stringProp name="Start users count burst">10</stringProp>
        <stringProp name="Start users period">30</stringProp>
        <stringProp name="Stop users count">${STEP_USERS}</stringProp>
        <stringProp name="Stop users period">30</stringProp>
        <stringProp name="flighttime">${STEP_DURATION}</stringProp>
        <stringProp name="rampUp">60</stringProp>
